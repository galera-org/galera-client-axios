{
  "compilerOptions": {
    "declaration": true,
    "target": "es6",
    "module": "es6",
    "moduleResolution": "node",
    "strict": true,
    "sourceMap": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "noImplicitAny": true,
    "skipLibCheck": true,
    "rootDir": ".",
    "outDir": "dist",
    "types": [
      "@types/node"
    ]
  },
  "exclude": ["node_modules", "dist"]
}
